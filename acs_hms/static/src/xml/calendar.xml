<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">
    <t t-name="calendarcheckout" t-inherit="web.CalendarCommonPopover.footer" t-inherit-mode="extension" owl="1">
         <xpath expr="//t[contains(@t-if,'isEventEditable')]" position="replace">
             <t t-if="isEventEditable">
                <a href="#" class="btn btn-primary o_cw_popover_edit" t-on-click="onEditEvent">Open</a>
                 <a style="margin-left: 10px;" href="#" class="btn btn-primary o_cw_popover_edit checkout-pos" t-on-click="onCheckout">Checkout</a>
            </t>
        </xpath>

    </t>

</templates>